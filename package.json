{
  "dependencies": {
    "cmake-js": "6.2.x",
    "nan": "2.15.x",
    "tslib": "2.x"
  },
  "scripts": {
    "test": "yarn build && NODE_ENV=development npx ts-node --project test/tsconfig.json test/test.ts",
    "debug": "npx cmake-js compile -C -D --out=build/debug",
    "prepublishOnly": "yarn test",
    "build":"./ImageMagick6-cmake/configure.sh && yarn debug && npx cmake-js compile -C --out=build/release",
    "postinstall": "yarn build"
  },
  "main": "./app/index.js",
  "typings": "./app/index.d.ts",
  "license": "MIT",
  "files": [
    "LICENSE",
    "CMakeLists.txt",
    "ImageMagick6",
    "src/**/*.{cpp,h}",
    "app/**/*.{js,d.ts}"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VictorQueiroz/image-m"
  },
  "keywords": [
    "imagemagick",
    "bindings",
    "native",
    "image-processing",
    "image"
  ],
  "author": {
    "name": "Victor Queiroz",
    "email": "victorcqueirozg@gmail.com",
    "url": "https://victorqueiroz.dev"
  },
  "devDependencies": {
    "@types/node": "^16.4.13",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.0",
    "typescript": "4.3.x"
  },
  "description": "ImageMagick 6 bindings for Node.js",
  "name": "image-m",
  "version": "0.1.1"
}
